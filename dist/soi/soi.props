#SOI Executive settings

#Nominal Speed
speed=1.0

#Maximum Depth
max_depth=4.0

#Minimum Depth
min_depth=0.0

#Maximum Speed
max_speed=1.5

#Minimum Speed
min_speed=0.7

#DUNE Host Address
host_addr=10.0.10.120

#DUNE Host Port (TCP)
host_port=6006

#Minutes before termination
mins_timeout=600

#Maximum time without reporting position
mins_offline=15

#Maximum time without GPS
mins_under=3

#Number where to send reports
sms_number=+351914785889

#Seconds to idle at each vertex
wait_secs=60

#SOI plan identifier
soi_plan_id=soi_plan

#Cyclic execution
cycle=false

#Speed up before descending
descendSpeedRpm=1300

#Upload temperature profiles when idle
uploadTemp=false

#Align with destination waypoint before going underwater
alignBeforeDive=true

